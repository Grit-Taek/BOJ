#include <cstdio>

bool visited[101];
int main()
{
    int n;
    scanf("%d" , &n);
    int cnt = 0;
    for(int i=0;i<n;i++)
    {
        int a;
        scanf("%d", &a);
        if(visited[a])
            cnt++;
        else
            visited[a] = true;
    }

    printf("%d", cnt);
    return 0;
}
