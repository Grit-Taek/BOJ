#include <cstdio>
#include <vector>
#include <algorithm>
using namespace std;
vector<int> v;
int main()
{

    for(int i=0;i<3;i++)
    {
        int a;
        scanf("%d", &a);
        v.push_back(a);
    }

    sort(v.begin() , v.end());
    for(int i=0;i<3;i++)
        printf("%d ", v[i]);

    return 0;
}
