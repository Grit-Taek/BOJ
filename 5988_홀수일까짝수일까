#include <iostream>
#include <string>

using namespace std;

int main()
{
    int n;
    cin >> n;
    for(int i=0;i<n;i++)
    {
        string str;
        cin >> str;
        int size = str.length();
        if((str[size-1] - '0') % 2 == 0 ) cout << "even" << endl;
        else cout << "odd" << endl;
    }

    return 0;
}

