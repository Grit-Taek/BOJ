#include <iostream>

int main()
{
	int h, m;
	scanf("%d %d", &h, &m);

	if (h == 0)
	{
		if (m - 45 < 0)
		{
			int a = 60 - 45 + m;
			h = 23;
			printf("%d %d", h, a);
		}
		else
		{
			int b = m - 45;
			printf("%d %d", h, b);
		}
	}
	
	else
	{
		if (m - 45 < 0)
		{
			int a = 60 - 45 + m;
			h -= 1;
			printf("%d %d", h, a);
		}
		else
		{
			int b = m - 45;
			printf("%d %d", h, b);
		}
	}

	return 0;

}
