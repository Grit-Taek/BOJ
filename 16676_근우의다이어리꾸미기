#include <iostream>

using namespace std;

int main()
{
	int n;
	scanf("%d", &n);
	int cnt = 0;
	if ((0 <= n) && (n <= 10))
		cnt = 1;
	else if ((11 <= n) && (n <= 110))
		cnt = 2;
	else if ((111 <= n) && (n <= 1110))
		cnt = 3;
	else if ((1111 <= n) && (n <= 11110))
		cnt = 4;
	else if ((11111 <= n) && (n <= 111110))
		cnt = 5;
	else if ((111111 <= n) && (n <= 1111110))
		cnt = 6;
	else if ((1111111 <= n) && (n <= 11111110))
		cnt = 7;
	else if ((11111111 <= n) && (n <= 111111110))
		cnt = 8;
	else if ((111111111 <= n) && (n <= 1111111110))
		cnt = 9;
	

	printf("%d", cnt);
	return 0;
}
