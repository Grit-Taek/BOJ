#include <cstdio>
#include <vector>
#include <algorithm>
using namespace std;

vector<int> v;
int main()
{
    int n,m;
    scanf("%d %d", &n,&m);
    for(int i=0;i<n;i++)
    {
        int a;
        scanf("%d",&a);
        v.push_back(a);
    }
    for(int i=0;i<m;i++)
    {
        int a;
        scanf("%d",&a);
        v.push_back(a);
    }
    sort(v.begin() , v.end());
    for(int i=0; i<v.size();i++)
        printf("%d ", v[i]);
    return 0;

}
